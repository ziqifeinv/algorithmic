    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            // "miDebuggerPath": "E:\\Qt\\Qt5.13.1\\Tools\\mingw730_64\\bin\\gdb.exe",
            "miDebuggerPath": "D:\\mingw\\bin\\gdb.exe",
            // "miDebuggerPath": "E:\\Microsoft\\AndroidNDK64\\android-ndk-r16b\\prebuilt\\windows-x86_64\\bin\\gdb.exe",
            "preLaunchTask": "gcc",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}

